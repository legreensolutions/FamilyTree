<div class="button"><%= link_to 'All', interesting_links_path %></div>
<div align="center">
<h3>Editing event</h3>
<table align="center">
<% form_for @interesting_link do |f| %>
  <%= f.error_messages %>

  <tr>
    <td>Title</td>
    <td><%= f.text_field :name %></td>
  </tr>
  <tr>
    <td>Description</td>
    <td><%= f.text_field :description %></td>
  </tr>


  <tr>
    <td colspan="2" align="center"><div class="button"><%= f.submit 'Update'%></div></td>
  </tr>
<% end %>
</table>
</div>

