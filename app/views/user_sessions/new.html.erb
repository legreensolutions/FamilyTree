<h1>Login</h1>

<% form_for @user_session, :url => user_session_path do |f| %>
  <%= f.error_messages %>
  <%= f.label :email %><br />
  <%= f.text_field :email %><br />
  <br />
  <%= f.label :password %><br />
  <%= f.password_field :password %><br />

  <br />
  <%= f.submit "Login" %>
<% end %>
<%= link_to 'Forgot Password?',:controller=>'password_resets',:action=>'new'%>

