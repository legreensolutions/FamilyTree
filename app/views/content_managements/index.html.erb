<div align="center">
<h3>Content Management</h3>


<table align="center">

<% @content_managements.each do |content_management| %>
   <tr>
    <td><%= link_to content_management.page, edit_content_management_path(content_management)%></td>
    <td><%= link_to image_tag('edit.png'), edit_content_management_path(content_management) %></td>
    <td><%= link_to image_tag('delete.png'), content_management, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />
</div>

