<div class="button"><%= link_to 'Back',members_path %></div>
<div style="width:65%;float:right;" >

        <h3>Committee Members</h3>
         <% @members.each do |member| %>
            <div style="float:left;" align="center">
              <% unless member.posts.nil?
                    member.posts.each do |post| %>
                     <%= post.name%>
                    <%end
                  end
              %> &nbsp;&nbsp;<br />
              <%#= member.post.name %> &nbsp;&nbsp;<br />
              <%= display_photo_of_member(member,member.name)%>
              <%# unless member.photo_file_name.nil? %>
              <%#= image_tag member.photo.url(:thumb),:title=>member.name %>&nbsp;<br />
              <%# else %>
              <%#= image_tag 'default_big.jpg',:title=>member.name %>&nbsp;<br />
              <%# end %>
            </div>

        <% end %>


    <br /><br />
    <div style="clear:both;"></div>

        <br /><br />

<div style="width:80%;text-align:justify;margin:auto;">
      <%#= @content_management.description[0..@content_management.no_of_chars] %>
      <%#= @content_management.description if @content_management %>
    </div>
</div>
<% unless @years.nil? %>
<div id="family_container"><%= render :partial=>'members/prev_committee_members' %></div>
<% end %>

