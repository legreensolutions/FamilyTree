<br />

<div class="page_container">
<h3>New member</h3>

<% form_for @member, :html => { :multipart => true } do |f| %>
  <%= f.error_messages %>
  <%= error_messages_for 'user'%>

<%= render :partial => "form", :object => f %>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>
</div>
<%= link_to 'Back', members_path %>

